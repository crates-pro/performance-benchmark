function_signature: (ModuledIdentifier, Params, Ty) = {
    <label: moduled_identifier> <left_parenthesis: left_parenthesis> <params: params> <right_parenthesis: right_parenthesis> "->" <ret_ty: ty>
        => (
            label,
            params,
            ret_ty,
        ),

    <label: moduled_identifier> <left_parenthesis: left_parenthesis> <right_parenthesis: right_parenthesis> "->" <ret_ty: ty>
        => (
            label,
            vec![],                                           
            ret_ty,
        ),

    <label: moduled_identifier> <left_parenthesis: left_parenthesis> <params: params> <right_parenthesis: right_parenthesis> "->" <ret_ty: trait_bounds_ty>
        => (
            label,
            params,
            ret_ty,
        ),        

    <label: moduled_identifier> <left_parenthesis: left_parenthesis> <right_parenthesis: right_parenthesis> "->" <ret_ty: trait_bounds_ty>
        => (
            label,
            vec![],                                           
            ret_ty,
        ),
    <op_struct: op_struct> <left_parenthesis: left_parenthesis> <params: params> <right_parenthesis: right_parenthesis> "->" <ret_ty: ty>
        => (
            vec![op_struct.to_string()],
